@model IEnumerable<Financeiro.Models.Source>

@{
    ViewBag.Title = "Index";
}

<h2>Gerenciar fonte de despesa</h2>

<p>
    <a href="/Source/create">Criar nova fonte de despesa</a> 
</p>
<table class="table">
    <tr>
        <th>
            Nome
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item.name
        </td>
        <td>
            <a href="/Source/edit/@item.id">Editar</a> |
            <a href="/Source/delete/@item.id">deletar</a> |
        </td>
    </tr>
}

</table>
